                                            <div class="col-md-9">
                                                <input class="form-control" type="number" name="num2" id="num2" placeholder="2" />
                                            </div>
                                        </div>
                                        <div class="form-group row rowHeight">
                                            <label class="col-md-3 col-form-label" for="numRunForm">Number 3: </label>
                                            <div class="col-md-9">
                                                <input class="form-control" type="number" name="num3" id="num3" placeholder="3" />
                                            </div>
                                        </div>
                                        <div class="form-group row rowHeight">
                                            <label class="col-md-3 col-form-label" for="numRunForm">Number 4: </label>
                                            <div class="col-md-9">
                                                <input class="form-control" type="number" name="num4" id="num4" placeholder="4" />
                                            </div>
                                        </div>
                                        <div class="form-group row rowHeight">
                                            <label class="col-md-3 col-form-label" for="numRunForm">Number 5: </label>
                                            <div class="col-md-9">
                                                <input class="form-control" type="number" name="num5" id="num5" placeholder="5" />
                                            </div>
                                        </div>
                                    </form>
                                    <div class="row rowHeight">
                                        <button id="btn1" class="btn btn-primary">Calculate</button>
                                        <button id="btnNumCode" class="btn btn-primary">Show Code <i class="fa fa-caret-up"></i></button>
                                    </div>
                                    <div class="code" id="numRunCode">
                                        <pre class="brush: js">
                                            $("#btn1").click(function () {
                                                $("#nbrRunResult").modal("show");
                                                $(document).on('hidden.bs.modal', function (event) {
                                                    if ($('.modal:visible').length) {
                                                        $('body').addClass('modal-open');
                                                    }
                                                });

                                                //accquire and calculate
                                                var num1 = Number($("#num1").val());
                                                var num2 = Number($("#num2").val());
                                                var num3 = Number($("#num3").val());
                                                var num4 = Number($("#num4").val());
                                                var num5 = Number($("#num5").val());

                                                var lessNum = Math.min(num1, num2, num3, num4, num5);
                                                $("#output1").html("The <b>smallest</b> number of " + num1 + ", " + num2 + ", " + num3 + ", " + num4 + ", " + num5 + " is <b>" + lessNum + "</b>");

                                                var greatNum = Math.max(num1, num2, num3, num4, num5);
                                                $("#output2").html("The <b>largest</b> number of " + num1 + ", " + num2 + ", " + num3 + ", " + num4 + ", " + num5 + " is <b>" + greatNum + "</b>");

                                                var meanNum = (num1 + num2 + num3 + num4 + num5) / 3;
                                                $("#output3").html("The <b>mean</b> of " + num1 + ", " + num2 + ", " + num3 + ", " + num4 + ", " + num5 + " is <b>" + meanNum + "</b>");

                                                var sum = num1 + num2 + num3 + num4 + num5;
                                                $("#output4").html("The <b>sum</b> of " + num1 + ", " + num2 + ", " + num3 + ", " + num4 + ", " + num5 + " is <b>" + sum + "</b>");

                                                var product = num1 * num2 * num3 * num4 * num5;
                                                $("#output5").html("The <b>product</b> of " + num1 + ", " + num2 + ", " + num3 + ", " + num4 + ", " + num5 + " is <b>" + product + "</b>");
                                            });
                                        </pre>
                                    </div>
                                </div>

                                <div id="facNum" class="tab-pane fade">
                                    <h3>Factorial of a Number</h3>
                                    <div class="rowHeight">
                                        <form action="" name="facForm" id="facForm">
                                            <div class="form-group row rowHeight">
                                                <label for="facForm" class="col-md-3 col-form-label">Enter Number: </label>
                                                <div class="col-md-9">
                                                    <input class="form-control" type="number" name="facIn" id="facIn" placeholder="1" />
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                    <button id="btn2" class="btn btn-primary">Calculate</button>
                                    <button id="btnFacCode" class="btn btn-primary">Show Code <i class="fa fa-caret-up"></i></button>

                                    <div class="code" id="facCode">
                                        <pre class="brush: js">
                                            $("#btn2").click(function () {
                                            $("#factorialResult").modal("show");
                                            $(document).on('hidden.bs.modal', function (event) {
                                                if ($('.modal:visible').length) {
                                                    $('body').addClass('modal-open');
                                                }
                                            });

                                            var facNum = Number($("#facIn").val());
                                            var newFacNum = facNum;

                                            if (newFacNum < 0) {
                                                $("#facOut").text("Enter a value greater than 0");
                                                return -1
                                            };

                                            var i;
                                            for (i = newFacNum - 1; i > 0; i--) {
                                                newFacNum *= i;
                                            }

                                            $("#facOut").html("The Factorial of <b>" + facNum + "</b> is: <b>" + newFacNum + "</b>");
                                            });
                                        </pre>
                                    </div>
                                </div>

                                <div id="fizzBuzz" class="tab-pane fade">
                                    <h3>FizzBuzz</h3>
                                    <h4>Enter two numbers between 1 and 100</h4>
                                    <form action="" name="fizzBuzzForm" id="fizzBuzzForm">
                                        <div class="form-group row rowHeight">
                                            <label for="fizzBuzzForm" class="col-md-3 col-form-label">Enter Fizz Number:</label>
                                            <div class="col-md-9">
                                                <input type="number" class="form-control" id="fizzNum1" placeholder="1">
                                            </div>
                                        </div>
                                        <div class="form-group row rowHeight">
                                            <label for="fizzBuzzForm" class="col-md-3 col-form-label">Enter Buzz Number:</label>
                                            <div class="col-md-9">
                                                <input type="number" class="form-control" id="fizzNum2" placeholder="2">
                                            </div>
                                        </div>
                                    </form>
                                    <button id="btn3" class="btn btn-primary">Show FizzBuzz</button>
                                    <button id="btnFizzCode" class="btn btn-primary">Show Code <i class="fa fa-caret-up"></i></button>
                                    <div class="code" id="fizzBuzzCode">
                                        <pre class="brush: js">
                                            $("#btn3").click(function () {
                                                $("#fizzBuzzResult").modal("show");
                                                $(document).on('hidden.bs.modal', function (event) {
                                                    if ($('.modal:visible').length) {
                                                        $('body').addClass('modal-open');
                                                    }
                                                });

                                                var fizzNum1 = Number($("#fizzNum1").val());
                                                var fizzNum2 = Number($("#fizzNum2").val());
                                                var results = [];

                                                var i = 0;
                                                for (i = 0; i < 100; i++) {
                                                    if ((i + 1) % fizzNum1 == 0 || (i + 1) % fizzNum2 == 0) {
                                                        if ((i + 1) % fizzNum1 == 0) { results = results.concat("<b>Fizz</b>"); }
                                                        if ((i + 1) % fizzNum2 == 0) { results = results.concat("<b>Buzz</b>"); }
                                                    } else {
                                                        results = results.concat((i + 1));
                                                    }
                                                    if (i < 99) { results = results.concat(", ") }
                                                };
                                                $("#fizzOut").html(results);
                                            });
                                        </pre>
                                    </div>
                                </div>

                                <div id="palindrome" class="tab-pane fade">
                                    <h3>Is it a Palindrome?</h3>
                                    <form action="" name="palForm" id="palForm">
                                        <div class="form-group row rowHeight">
                                            <label for="palForm" class="col-md-3 col-form-label">Enter a Word: </label>

                                            <div class="col-md-9">
                                                <input class="form-control" type="text" name="palIn" id="palIn" placeholder="Palindrome" />
                                            </div>
                                        </div>
                                    </form>
                                    <button id="btn4" class="btn btn-primary">Press and See</button>
                                    <button id="btnPalCode" class="btn btn-primary">Show Code <i class="fa fa-caret-up"></i></button>
                                    <div class="code" id="palCode">
                                        <pre class="brush: js">
                                            $("#btn4").click(function () {
                                                $("#palResults").modal("show");
                                                $(document).on('hidden.bs.modal', function (event) {
                                                    if ($('.modal:visible').length) {
                                                        $('body').addClass('modal-open');
                                                    }
                                                });

                                                var testWord = $("#palIn").val();
                                                var newWord = testWord.split("").reverse();
                                                var response;

                                                if (testWord.toUpperCase() === newWord.toString().toUpperCase().replace(/,/g, '')) { response = "Your word <b>" + testWord + "</b> is a palindrome!" }
                                                else { response = "Sorry!, your word <b>" + testWord + "</b> is not a palindrome." }

                                                $("#palOut").html(response);
                                            });
                                        </pre>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times"></i> Close</button>
                    </div>
